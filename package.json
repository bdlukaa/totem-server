{
  "name": "theme",
  "version": "3.5.0",
  "description": "",
  "type": "module",
  "scripts": {
    "build:atcb": "bun build node_modules/add-to-calendar-button/dist/atcb-no-pro.js --drop=console --minify --outfile=totem/static/js/atcb.min.js && perl -pi -e s,add-to-calendar-pro.com,totem.org,g totem/static/js/atcb.min.js",
    "build:js": "./build.ts",
    "build:tailwind": "NODE_ENV=production bun run tw -- --minify",
    "build": "concurrently \"bun run build:tailwind\" \"bun run build:js\" \"bun run build:atcb\"",
    "dev": "concurrently --kill-others \"bun run dev:js\" \"bun run dev:tailwind\"",
    "livereload": "livereload ./totem/ -e 'html,css,js,py,mjml' --wait 200",
    "openapi-ts": "openapi-ts",
    "start": "bun run dev",
    "tw": "tailwindcss -i ./assets/css/styles.css -o ./totem/static/js/bundles/styles.css",
    "dev:js": "./build.ts dev",
    "dev:tailwind": "NODE_ENV=development bun run tw",
    "test": "vitest",
    "test:ci": "vitest run"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "bun": "<2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/js": "^9.22.0",
    "@hey-api/openapi-ts": "^0.64.10",
    "@solidjs/testing-library": "^0.8.10",
    "@tailwindcss/cli": "^4.0.12",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/eslint-plugin-query": "^5.67.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.2.4",
    "@types/js-cookie": "^3.0.6",
    "bun-plugin-solid": "^1.0.0",
    "concurrently": "^9.1.2",
    "daisyui": "^5.0.0",
    "eslint": "^9.22.0",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "jsdom": "^26.0.0",
    "livereload": "^0.9.3",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-jinja-template": "^2.0.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.12",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "vite-plugin-solid": "^2.11.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.8.3",
    "@leeoniya/ufuzzy": "^1.0.18",
    "@rnwonder/solid-date-picker": "^1.4.1",
    "@solid-primitives/intersection-observer": "^2.2.0",
    "@solid-primitives/media": "^2.3.0",
    "@solid-primitives/refs": "^1.1.0",
    "@tanstack/solid-query": "^5.67.2",
    "@totem.org/solid-boring-avatars": "^0.0.2",
    "@zootools/email-spell-checker": "^1.12.0",
    "add-to-calendar-button": "^2.8.6",
    "js-cookie": "^3.0.5",
    "solid-element": "^1.9.1",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.9.5",
    "tippy.js": "^6.3.7"
  }
}
