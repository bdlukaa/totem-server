{
  "name": "theme",
  "version": "3.5.0",
  "description": "",
  "scripts": {
    "start": "npm run dev",
    "build": "npm run build:tailwind && npm run build:js",
    "build:js": "esbuild --bundle --sourcemap --define:process.env.NODE_ENV=\"\\\"production\\\"\" ./assets/js/app.js --outfile=./totem/static/js/app.min.js --minify",
    "build:tailwind": "cross-env NODE_ENV=production tailwindcss --postcss -i ./assets/css/styles.css -o ./totem/static/css/styles.css --minify",
    "dev": "concurrently --kill-others \"npm run watch:js\" \"npm run watch:tailwind\"",
    "watch:js": "esbuild --bundle --watch --sourcemap --define:process.env.NODE_ENV=\"\\\"development\\\"\" ./assets/js/app.js --outfile=./totem/static/js/app.min.js",
    "watch:tailwind": "cross-env NODE_ENV=development tailwindcss --postcss -i ./assets/css/styles.css -o ./totem/static/css/styles.css -w --minify",
    "tailwindcss": "node ./node_modules/tailwindcss/lib/cli.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.2",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.18.17",
    "postcss": "^8.4.14",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.5.2",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.2.7"
  },
  "dependencies": {
    "@zootools/email-spell-checker": "^1.12.0",
    "add-to-calendar-button": "^2.3.1"
  }
}
