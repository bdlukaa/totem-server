{
  "name": "Totem Web Server",
  "healthchecks": {
    "web": [
      {
        "type": "startup",
        "name": "web check",
        "path": "/",
        "contents": "Totem"
      }
    ]
  },
  "cron": [
    {
      "command": "./manage.py totem_tasks",
      "schedule": "*/10 * * * *"
    }
  ]
}
